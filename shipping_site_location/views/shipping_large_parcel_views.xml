<odoo>
    <record id="shipping_large_parcel_form_view" model="ir.ui.view">
        <field name="name">shipping_large_parcel_form_view</field>
        <field name="model">shipping.large.parcel</field>
        <field name="inherit_id" ref="shipping_bills.shipping_large_parcel_form"/>
        <field name="priority" eval="37"/>
        <field name="arch" type="xml">
            <field name="shipping_bill_ids" position="attributes">
                <attribute name="domain">['|', '&amp;', ('state', '=', 'valued'), ('sale_site_id', '=', site_id), '&amp;', ('partner_vip_type', '=', 'svip'),  ('state', '=', 'paired'), ('sale_site_id', '=', site_id)]</attribute>
            </field>
        </field>
    </record>
</odoo>