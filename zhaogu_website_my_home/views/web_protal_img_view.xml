<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="web_protal_img_tree" model="ir.ui.view">
        <field name="name">web_protal_img_tree</field>
        <field name="model">web.protal.img</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="banner_position"/>
                <field name="url"/>
            </tree>
        </field>
    </record>

    <record id="web_protal_img_form" model="ir.ui.view">
        <field name="name">web_protal_img_form</field>
        <field name="model">web.protal.img</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <group>
                        <field name="name"/>
                        <field name="url"/>
                    </group>
                    <group>
                        <field name="banner_img" widget="image"/>
                        <field name="banner_position"/>
                    </group>
                </group>
            </form>
        </field>
    </record>

</odoo>
