<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="res_partner_form" model="ir.ui.view">
            <field name="name">res_partner_form</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <page name="sales_purchases" position="inside">
                    <group name="agency">
                        <group>
                            <field name="is_state_agency"/>
                            <field name="is_city_agency"/>
                            <field name="is_customer"/>
                        </group>
                        <group>
                            <field name="state_agency_id"/>
                            <field name="city_agency_ids" widget="many2many_tags"/>
                            <field name="city_agency_id"/>
                            <field name="customer_ids" widget="many2many_tags"/>
                        </group>
                    </group>
                </page>
            </field>
        </record>


        <record id="res_partner_form3" model="ir.ui.view">
            <field name="name">res_partner_form</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="sales_team.res_partner_view_team"/>
            <field name="arch" type="xml">
                <field name="team_id" position="after">
                    <field name="team_stock_location_id"/>
                </field>
            </field>
        </record>





    </data>
</odoo>
